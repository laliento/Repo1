<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.com.walmart.fyd.model.PermisoMenu" table="REL_PERMISO_MENU">
        <id name="id" type="java.lang.Integer">
            <column name="ID" sql-type ="smallint"/>
            <generator class="identity"/>
        </id>
        <many-to-one name="menu" class="mx.com.walmart.fyd.model.Menu">
        	<column name="ID_MENU" not-null="true" sql-type ="smallint"/>
        </many-to-one>
        <property name="idPerfil" type="java.lang.Integer">
        	<column name="ID_PERFIL" not-null="true" sql-type ="smallint"/>
        </property>
    </class>
</hibernate-mapping>
